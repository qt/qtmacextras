[project]
	description = The Qt MacExtras repository\n\
\n\
ssh://codereview.qt-project.org:29418/playground/qtmacextras.git
[access "refs/*"]
	read = deny group Gitorious Mirror Bot
[access]
	inheritFrom = All-Non-CI-Playground-Projects
[receive]
	requireContributorAgreement = true
	requireChangeId = true
[cherryPick]
	includeReviewedOn = true
[submit]
	action = cherry pick
	mergeContent = true
[cherryPick "categoryFooters"]
	Sanity-Review = true
